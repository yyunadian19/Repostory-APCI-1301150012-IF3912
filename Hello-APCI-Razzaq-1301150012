Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301150012’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘YUSUF yUNADIAN’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘IF-39-12’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘RAZZAQ’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154222’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.